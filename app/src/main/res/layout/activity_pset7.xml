<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    tools:context="com.example.practiceset5.pset7">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:src="@drawable/musicsymbol1"
        android:id="@+id/musicsymbol1"
        android:onClick="playmusic1"
        android:layout_margin="16dp"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="100dp"
        android:src="@drawable/musicsymbol3"
        android:id="@+id/musicsymbol2"
        android:onClick="playmusic2"
        android:layout_margin="16dp" />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:src="@drawable/musicsymbol2"
        android:layout_margin="16dp"
        android:id="@+id/musicsymbol3"
        android:onClick="playmusic3"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="100dp"
        android:src="@drawable/musicsymbol4"
        android:layout_margin="16dp"
        android:id="@+id/musicsymbol4"
        android:onClick="playmusic4"/>


</LinearLayout>
